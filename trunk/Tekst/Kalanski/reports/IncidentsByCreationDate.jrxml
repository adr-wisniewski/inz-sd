	<queryString>
		<![CDATA[select
  i.subject
  , extract(year from i.creation_date) as rok
  , to_char(i.creation_date, 'month') as miesiac
  , extract(year from i.creation_date) * 100 + extract(month from i.creation_date) as rok_miesiac
  , p.name as priority
  , c.name as category
  , e.name || ' ' || e.surname as author
  , i.creation_date
from
  incidents i
  join priority_types_im p on p.priority_code = i.priority_code
  join categories_im c on c.category_code = i.category_code
  join employees e on e.employee_id = i.author
order by  i.creation_date, i.category_code]]>
	</queryString>
